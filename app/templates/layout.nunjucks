<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">

        <!-- Enable responsiveness on mobile devices-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1">

        <title>{% block title %}{% endblock %} | The brown Fox</title>

        <!-- CSS -->
        <link rel="stylesheet" href="design/css/brownfox.css">

        <!-- Icons -->
        <link rel="apple-touch-icon" sizes="180x180" href="design/icons/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="design/icons/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="design/icons/favicon-16x16.png">
        <link rel="manifest" href="design/icons/manifest.json">
        <link rel="mask-icon" href="design/icons/safari-pinned-tab.svg" color="#5bbad5">
        <link rel="shortcut icon" href="design/icons/favicon.ico">
        <meta name="msapplication-config" content="design/icons/browserconfig.xml">
        <meta name="theme-color" content="#f06322">
    </head>
    <body>
        {% block content %} {% endblock %}
        <script src="design/js/lib.js"></script>
        <script src="design/js/applib.js"></script>
        <script>
            (function(i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function() {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-98350485-2', 'auto');
            ga('send', 'pageview');
        </script>
    </body>
</html>
